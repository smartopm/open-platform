<table class="table">
  <thead>
    <tr>
      <th scope="col">Date</th>
      <th scope="col">Time</th>
      <th scope="col">Reporter</th>
      <th scope="col">Notes</th>
    </tr>
  </thead>
  <tbody>
    <%- @activity_logs.each do |al| %>
      <tr>
        <td><%= al.created_at.strftime("%Y-%m-%d") -%></td>
        <td><%= al.created_at.strftime("%H:%M") -%></td>
        <td><%= al.reporting_member.user.name -%></td>
        <td><%= al.note -%></td>
      </tr>
    <%- end %>
  </tbody>
</table>
